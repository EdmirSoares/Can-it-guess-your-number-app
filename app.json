{
	"expo": {
		"name": "mini-game-app",
		"slug": "mini-game-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/splash-teste.png",
		"userInterfaceStyle": "light",
		"splash": {
			"backgroundColor": "#0b0b0b",
			"resizeMode": "contain"
		},
		"ios": {
			"supportsTablet": true,
			"newArchEnabled": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/splash-teste.png",
				"backgroundColor": "#0b0b0b"
			},
			"newArchEnabled": true
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-font"
		]
	}
}